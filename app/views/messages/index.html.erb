<h1>Messages#index</h1>
<p>Find me in app/views/messages/index.html.erb</p>
<table border="1">
<th>Name</th>
<th>Content</th>
<th>Created at</th>
<th>Updated at</th>
	
<% @messages.each do |f| %>
<tr>
<td><%= f.name %></td>
<td><%= f.content %></td>
<td><%= f.created_at %></td>
<td><%= f.updated_at %></td>
</tr>
<% end %>

</table>
